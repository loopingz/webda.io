{
  "name": "@webda/core",
  "version": "1.2.1",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts src/*.spec.ts"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.8",
    "@webda/workout": "^1.2.1",
    "accept-language": "^3.0.18",
    "ajv": "^8.6.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "dateformat": "^4.5.1",
    "deepmerge": "^4.2.2",
    "email-templates": "^8.0.7",
    "fs-finder": "^1.8.1",
    "global": "^4.4.0",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "merge": "^2.1.1",
    "mime-types": "^2.1.31",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.3",
    "openapi-types": "^9.1.0",
    "sanitize-html": "^2.4.0",
    "semver": "^7.3.5",
    "ts-method-cache": "^3.3.1",
    "uri-templates": "^0.2.0",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@testdeck/mocha": "^0.1.2",
    "@types/node": "^16.4.2",
    "axios": "^0.21.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^11.1.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "~4.4.3"
  },
  "files": [
    "lib",
    "test"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "exclude": [
      "src/test.ts",
      "**/*.spec.ts",
      "test/**/*"
    ]
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "webda": {
    "namespace": "Webda"
  }
}
