{
  "name": "@webda/compiler",
  "version": "4.0.0-beta.1",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "bin": {
    "webdac": "./lib/shell.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc-esm",
    "build:watch": "tsc-esm --watch",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "schema": "ts-json-schema-generator --path src/definition.ts -f ./tsconfig.json --type 'WebdaModule' -o ./webda.module.schema.json",
    "pretest": "yarn build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.8",
    "@webda/workout": "^4.0.0-beta.1",
    "accept-language": "^3.0.18",
    "ts-json-schema-generator": "^2.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "@webda/tsc-esm": "^4.0.0-beta.1",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "skott": "^0.35.3",
    "vitest": "^2.1.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "LGPL-3.0-only"
}
