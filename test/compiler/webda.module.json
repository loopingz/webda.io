    "Webda/AnotherSchema": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string",
          "description": "Contact firstname"
        },
        "lastName": {
          "type": "string",
          "description": "Contact lastname"
        },
        "type": {
          "type": "string",
