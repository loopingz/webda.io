{
  "name": "@webda/cloudevents",
  "version": "4.0.0-beta.2",
  "description": "Discovery and Subscriptions for CloudEvents",
  "keywords": [
    "cloudevents",
    "webda"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc-esm",
    "build:watch": "tsc-esm --watch",
    "grammar": "antlr4ts -visitor src/stores/webdaql/WebdaQLLexer.g4 src/stores/webdaql/WebdaQLParser.g4 && yarn run lint:fix",
    "pretest": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest",
    "test:bun": "bun test --coverage",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "dependencies": {
    "cloudevents": "^8.0.1",
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "@webda/tsc-esm": "4.0.0-beta.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.17.0",
    "skott": "^0.35.3",
    "jscodeshift": "^17.0.0",
    "@webda/compiler": "^4.0.0-beta.2"
  },
  "files": [
    "lib"
  ],
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "src/test.ts",
      "**/*.spec.ts",
      "test/**/*",
      "*/stores/webdaql/WebdaQL*"
    ],
    "excludeNodeModules": true
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "LGPL-3.0-only"
}