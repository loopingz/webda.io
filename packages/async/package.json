{
  "name": "@webda/async",
  "version": "3.7.1",
  "description": "Async System",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc-esm",
    "build:module": "webda build",
    "build:watch": "webda build --watch",
    "pretest": "npm run build",
    "test": "c8 mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts src/*.spec.ts",
    "test:debug": "mocha --recursive --exit --no-timeouts 'src/**/*.spec.ts'",
    "run": "node core"
  },
  "keywords": [
    "webda-module"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "dependencies": {
    "@webda/core": "^3.15.1",
    "@webda/workout": "^3.2.0",
    "axios": "^1.1.3",
    "json-schema": "^0.4.0",
    "node-cron": "^3.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.3",
    "@types/node": "18.11.13",
    "@webda/tsc-esm": "^1.3.0",
    "mocha": "^10.0.0",
    "sinon": "^18.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.0",
    "typescript": "~5.5.2"
  },
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "webda": {
    "namespace": "Webda"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "nx": {
    "implicitDependencies": [
      "@webda/core",
      "@webda/tsc-esm"
    ]
  }
}
