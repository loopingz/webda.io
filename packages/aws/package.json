    "exclude": [
      "**/*.spec.ts",
      "test/**/*"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/aws-lambda": "^8.10.97",
