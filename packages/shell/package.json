{
  "name": "@webda/shell",
  "version": "2.0.0-beta.2",
  "description": "Deploy a Webda app or configure it",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git://github.com/loopingz/webda.io.git",
  "bin": {
    "webda": "./bin/webda"
  },
  "files": [
    "bin/webda",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts src/*.spec.ts",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.15.0",
    "@phenomnomnominal/tsquery": "^5.0.0",
    "@webda/core": "^2.0.0-beta.2",
    "@webda/kubernetes": "^2.0.0-beta.2",
    "@webda/workout": "^2.0.0-beta.2",
    "archiver": "^5.3.0",
    "cli-progress": "^3.9.0",
    "colors": "=1.4.0",
    "filesize": "^8.0.0",
    "fs-extra": "^10.0.0",
    "fs-finder": "^1.8.1",
    "generator-webda": "^1.2.0",
    "glob": "^8.0.1",
    "iam-policy-optimizer": "^1.1.2",
    "is-in-subnet": "^4.0.1",
    "json-stable-stringify": "^1.0.1",
    "merge": "^2.1.1",
    "mime-types": "^2.1.31",
    "nodejs-websocket": "^1.7.2",
    "open": "^8.2.1",
    "semver": "^7.3.5",
    "semver-intersect": "^1.4.0",
    "socket.io": "^4.1.3",
    "ts-json-schema-generator": "~1.0.0",
    "typescript": "~4.6.4",
    "unzipper": "^0.10.11",
    "yaml": "^2.0.0",
    "yamljs": "^0.3.0",
    "yargs": "^17.0.1",
    "yauzl": "^2.10.0",
    "yeoman-environment": "^3.5.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts",
      "link_modules/**/*",
      "test/**"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@testdeck/mocha": "^0.2.0",
    "@types/node": "14.0.0",
    "@types/yargs": "^17.0.2",
    "csstype": "^3.0.8",
    "sinon": "^14.0.0"
  }
}
