{
  "$schema": "./.webda-config-schema.json",
  "version": 3,
  "services": {
    "memorylogger": {
      "type": "MemoryLogger"
    },
    "images": {
      "type": "FileBinary",
      "folder": "./data/images",
      "map": {
        "contacts": [
          "picture"
        ]
      }
    },
    "thumbnailsqueue": {
      "type": "MemoryQueue"
    },
    // Contact store
    "contacts": {
      "model": "WebdaDemo/Contact",
      "type": "MemoryStore",
      "table": "local-table",
      "expose": {
        "url": "/contacts"
      }
    }
  },
