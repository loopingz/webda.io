{
  "name": "@webda/aws",
  "version": "0.11.1",
  "description": "Webda AWS Services implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc && webda module",
    "pretest": "npm run build",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ts-node/register src/**/*.spec.ts",
    "run": "node core",
    "format": "node ./scripts/format.js",
    "precommit": "npm run format && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "author": "loopingz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "dependencies": {
    "@webda/core": "^1.0.0",
    "aws-sdk": "^2.311.0",
    "uuid": "^3.3.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ]
  }
}
