{
  "name": "@webda/otel",
  "version": "3.3.0",
  "description": "OpenTelemetry module",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "webda build",
    "pretest": "npm run build",
    "test": "c8 mocha --recursive --exit --timeout=100000 src/**/*.spec.ts",
    "test:debug": "mocha --recursive --exit --timeout=100000 src/**/*.spec.ts",
    "new-model": "webda init webda:model",
    "new-service": "webda init webda:service"
  },
  "keywords": [
    "webda-module"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/api-logs": "^0.50.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.50.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.50.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.50.0",
    "@opentelemetry/instrumentation": "^0.50.0",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-logs": "^0.50.0",
    "@opentelemetry/sdk-metrics": "^1.18.1",
    "@opentelemetry/sdk-node": "^0.50.0",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "@types/node": "^18.0.0",
    "@webda/core": "^3.15.0",
    "@webda/workout": "^3.2.0"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.3",
    "@webda/shell": "^3.10.0",
    "c8": "^9.0.0",
    "glob": "^10.3.10",
    "js-beautify": "^1.14.9",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "c8": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "exclude": [
      "**/*.spec.ts",
      "test/**/*"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "repository": "git://github.com/loopingz/webda.io.git",
  "license": "MIT"
}
