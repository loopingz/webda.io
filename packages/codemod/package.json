{
  "name": "@webda/codemod",
  "version": "3.999.0",
  "description": "Migration helper",
  "keywords": [
    "webda"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "homepage": "https://github.com/loopingz/webda.io/tree/master/packages/codemod#readme",
  "license": "LGPL-3.0-only",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "*.js",
    "*.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "scripts": {
    "test": "vitest",
    "test:set": "UPDATE_SNAPSHOT=1 vitest",
    "format": "prettier --check src/**/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts"
  },
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "jscodeshift-add-imports": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "jscodeshift": "^17.0.0",
    "openapi-types": "^12.0.0",
    "skott": "^0.35.3",
    "tsx": "^4.17.0",
    "vitest": "^2.1.3",
    "@webda/compiler": "^4.0.0-beta.2"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}
