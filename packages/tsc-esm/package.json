{
  "name": "@webda/tsc-esm",
  "version": "4.0.0-beta.1",
  "description": "Compile with ES6 Module import correction",
  "keywords": [
    "webda"
  ],
  "bin": "./lib/bin.js",
  "type": "module",
  "author": "Remi Cattiau <remi@cattiau.com>",
  "homepage": "https://github.com/loopingz/webda.io/tree/master/packages/codemod#readme",
  "license": "LGPL-3.0-only",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "main": "./lib/lib.js",
  "exports": {
    "default": "./lib/lib.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "scripts": {
    "build": "tsc && chmod +x lib/bin.js",
    "format": "prettier --check src/**/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest debug",
    "test:coverage": "vitest run --coverage"
  },
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "typescript": "^5.6.2",
    "@webda/test": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "vitest": "^3.2.4",
    "openapi-types": "^12.0.0",
    "tsx": "^4.17.0",
    "skott": "^0.35.3",
    "jscodeshift": "^17.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
