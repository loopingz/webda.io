{
  "name": "@webda/shell",
  "version": "2.4.2",
  "description": "Deploy a Webda app or configure it",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git://github.com/loopingz/webda.io.git",
  "bin": {
    "webda": "./lib/bin/webda.js"
  },
  "files": [
    "lib",
    "webda.module.json"
  ],
  "scripts": {
    "build": "tsc-esm && chmod +x lib/bin/webda.js",
    "build:module": "webda build",
    "build:watch": "webda build --watch",
    "pretest": "npm run build",
    "test": "c8 mocha --recursive --exit --timeout=100000 src/**/*.spec.ts src/*.spec.ts",
    "test:debug": "mocha --recursive --exit --timeout=100000 src/**/*.spec.ts src/*.spec.ts",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.18.0",
    "@phenomnomnominal/tsquery": "^5.0.0",
    "@webda/core": "^2.4.2",
    "@webda/kubernetes": "^2.4.2",
    "@webda/tsc-esm": "^1.0.2",
    "@webda/workout": "^2.3.0",
    "archiver": "^5.3.0",
    "chalk": "^5.0.1",
    "cli-progress": "^3.9.0",
    "colors": "=1.4.0",
    "dateformat": "^5.0.3",
    "filesize": "^10.0.5",
    "fs-extra": "^11.0.0",
    "generator-webda": "^1.2.0",
    "glob": "^9.2.1",
    "is-in-subnet": "^4.0.1",
    "json-stable-stringify": "^1.0.1",
    "merge": "^2.1.1",
    "mime-types": "^2.1.31",
    "nodejs-websocket": "^1.7.2",
    "open": "^9.0.0",
    "semver": "^7.3.5",
    "semver-intersect": "^1.4.0",
    "socket.io": "^4.1.3",
    "ts-json-schema-generator": "~1.2.0",
    "typescript": "~4.9.3",
    "unzipper": "^0.10.11",
    "yaml": "^2.0.0",
    "yamljs": "^0.3.0",
    "yargs": "^17.0.1",
    "yauzl": "^2.10.0",
    "yeoman-environment": "^3.5.1"
  },
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts",
      "link_modules/**/*",
      "test/**"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/node": "14.6.0",
    "@types/yargs": "^17.0.2",
    "csstype": "^3.0.8",
    "sinon": "^15.0.0",
    "ts-node": "^10.8.1"
  },
  "type": "module"
}
