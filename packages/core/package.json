{
  "name": "@webda/core",
  "version": "1.1.0",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts src/*.spec.ts"
  },
  "dependencies": {
    "@webda/workout": "^1.0.0",
    "accept-language": "3.0.18",
    "ajv": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "cookie": ">=0.2.3",
    "dateformat": "^4.0.0",
    "deepmerge": "^4.2.2",
    "email-templates": "^5.0.4",
    "fs-finder": "^1.8.1",
    "glob": "^7.1.6",
    "global": "^4.4.0",
    "jsonpath": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "merge": "^2.1.1",
    "mime-types": "^2.1.24",
    "mustache": "^4.2.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.1.1",
    "sanitize-html": "^2.3.2",
    "ts-method-cache": "^3.3.1",
    "uri-templates": ">=0.2.0",
    "uuid": "^8.0.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@testdeck/mocha": "^0.1.0",
    "@types/json-schema": "^7.0.3",
    "@types/node": "^12.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": ">=6.1.4",
    "nyc": "^15.0.1",
    "openapi-types": "^1.3.5",
    "prettier": "^2.0.5",
    "remap-istanbul": "^0.13.0",
    "sinon": "^9.0.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "typescript": "^4.1.0"
  },
  "files": [
    "lib",
    "test"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ]
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "webda": {
    "namespace": "Webda"
  }
}
