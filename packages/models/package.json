{
  "name": "@webda/models",
  "version": "4.0.0-beta.1",
  "description": "Webda Models implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc-esm",
    "build:watch": "tsc-esm --watch",
    "pretest": "tsc-esm",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --no-trackTypeOnlyDependencies --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda",
    "modda"
  ],
  "files": [
    "lib",
    "package.json"
  ],
  "author": "loopingz",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "dependencies": {
    "@types/mocha": "^10.0.0"
  },
  "peerDependencies": {
    "@webda/ql": "^4.0.0-beta.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "22.0.0",
    "@webda/serialize": "^4.0.0-beta.1",
    "@webda/test": "^4.0.0-beta.1",
    "@webda/tsc-esm": "^4.0.0-beta.2",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "skott": "^0.35.4",
    "type-fest": "^5.0.1"
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  }
}
