{
  "name": "@webda/models",
  "version": "4.0.0-beta.1",
  "description": "Webda Models implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "webdac build",
    "build:watch": "webdac build --watch",
    "pretest": "webdac build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "c8 mocha",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda",
    "modda"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "webda.shell.json",
    "package.json"
  ],
  "author": "loopingz",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "dependencies": {
    "@types/mocha": "^10.0.0"
  },
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts",
      "test/**/*"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/node": "20.0.0",
    "@webda/compiler": "^4.0.0-beta.2",
    "@webda/tsc-esm": "^4.0.0-beta.2"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}
