{
  "name": "webda",
  "version": "0.11.0",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "pretest": "tsc",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ts-node/register test/**/*.test.js src/**/*.spec.ts",
    "run": "node core",
    "format": "node ./scripts/format.js",
    "precommit": "npm run test"
  },
  "dependencies": {
    "accept-language": "3.0.18",
    "ajv": "^6.10.0",
    "cookie": ">=0.2.3",
    "email-templates": "^5.0.4",
    "fs-finder": "^1.8.1",
    "google-auth-library": "^4.0.0",
    "jsonpath": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.2",
    "mime-types": "^2.1.24",
    "mustache": "^3.0.1",
    "nodemailer": "^6.1.1",
    "nodemailer-ses-transport": "^1.3.0",
    "passport": "=0.4.0",
    "passport-amazon": "^1.0.0",
    "passport-facebook": "^3.0.0",
    "passport-github2": ">=0.1.9",
    "passport-google-oauth": "^2.0.0",
    "passport-oauth": "^1.0.0",
    "passport-twitter": ">=1.0.3",
    "uri-templates": ">=0.2.0",
    "uuid": "^3.0.0"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/node": "^11.13.6",
    "glob": "^7.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^6.1.4",
    "mocha-typescript": "^1.1.17",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "remap-istanbul": "^0.13.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.4"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ]
  },
  "homepage": "https://webda.io"
}
