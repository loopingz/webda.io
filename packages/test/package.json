{
  "name": "@webda/test",
  "version": "4.0.0-beta.1",
  "description": "Test for NodeJS projects",
  "keywords": [],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "pretest": "tsc",
    "precommit": "eslint --fix && prettier --write src/**/*",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest debug",
    "test:coverage": "vitest --coverage",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "dependencies": {
    "@webda/workout": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jscodeshift": "^17.0.0"
  },
  "peerDependenciesMeta": {
    "mocha": {
      "required": false
    },
    "jest": {
      "required": false
    },
    "vitest": {
      "required": false
    }
  },
  "files": [
    "lib"
  ],
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "LGPL-3.0-only"
}
