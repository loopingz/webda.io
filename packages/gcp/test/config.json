{
  "version": 2,
  "parameters": {
    "accessKeyId": "Bouzouf",
    "secretAccessKey": "Plop",
    "TEST": "Global",
    "region": "us-east-1",
    "website": ["test.webda.io", "test2.webda.io"],
    "csrfOrigins": ["^accounts\\.google\\.\\w{2,}$", "www\\.facebook\\.com"],
    "locales": ["es-ES", "en-GB", "fr-FR"],
    "sessionSecret": ""
  },
  "module": {},
  "services": {
    "ConsoleLogger": {
      "type": "Webda/ConsoleLogger",
      "logLevel": "INFO"
    },
    "ConsoleTestLogger": {
      "type": "Webda/ConsoleLogger",
      "logLevels": "TEST",
      "logLevel": "TEST"
    },
    "Users": {
      "expose": {
        "url": "/users"
      },
      "type": "MemoryStore",
      "model": "Webda/User"
    },
    "binary": {
      "type": "Webda/GoogleCloudStorage",
      "bucket": "webda-dev",
      "expose": {
        "url": "/binary"
      },
      "map": {
        "Users": ["images"]
      }
    },
    "pubsub": {
      "type": "Webda/GoogleCloudPubSub",
      "topic": "unit-tests"
    },
    "queue": {
      "type": "Webda/GoogleCloudQueue",
      "topic": "unit-tests",
      "subscription": "queue",
      "timeout": 30000
    },
    "IdentsMapper": {
      "type": "Webda/Mapper",
      "source": "fireidents",
      "targetAttribute": "idents",
      "target": "fireusers",
      "attribute": "_user",
      "fields": "type,_lastUpdate,counter",
      "cascade": true
    },
    "fireidents": {
      "asyncDelete": true,
      "expose": {
        "restrict": {
          "create": true,
          "update": true,
          "get": true
        }
      },
      "model": "Webda/Ident",
      "type": "GoogleCloudFireStore",
      "collection": "idents"
    },
    "fireusers": {
      "expose": {
        "url": "/fireusers",
        "restrict": {
          "authentication": true,
          "create": true
        }
      },
      "type": "GoogleCloudFireStore",
      "model": "Webda/User",
      "collection": "users"
    }
  }
}
