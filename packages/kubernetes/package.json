{
  "name": "@webda/kubernetes",
  "version": "2.1.1",
  "description": "Kubernetes Helpers",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc-esm",
    "build:module": "webda build",
    "pretest": "npm run build",
    "test": "c8 mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts",
    "run": "node core"
  },
  "keywords": [
    "webda-module"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "dependencies": {
    "@kubernetes/client-node": "^0.16.3",
    "@webda/async": "^2.1.1",
    "@webda/core": "^2.1.1",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/node": "14.6.0",
    "c8": "^7.11.3",
    "glob": "^8.0.1",
    "js-beautify": "^1.14.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.0",
    "typescript": "~4.8.2"
  },
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "webda": {
    "namespace": "Webda"
  },
  "type": "module",
  "engines": {
    "node": ">=14.6.0"
  }
}
