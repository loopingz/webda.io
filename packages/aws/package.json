{
  "name": "@webda/aws",
  "version": "1.1.1",
  "description": "Webda AWS Services implementation",
  "main": "lib/index.js",
  "scripts": {
    "webda-module": "webda module --noCompile",
    "build": "tsc",
    "pretest": "tsc",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda",
    "modda"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "author": "loopingz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "dependencies": {
    "@webda/core": "^1.1.0",
    "aws-sdk": "^2.678.0",
    "bluebird": "^3.7.2",
    "glob": "^7.1.6",
    "iam-policy-optimizer": "^1.1.2",
    "uuid": "^8.0.0",
    "yaml": "^1.10.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.1.0",
    "@webda/shell": "^1.0.2",
    "aws-sdk-mock": "^5.1.0",
    "sinon": "^9.0.1"
  }
