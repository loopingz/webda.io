{
  "name": "@webda/core",
  "version": "4.0.0-beta.2",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "webdac build",
    "build:watch": "webdac build --watch",
    "deps:graph": "skott --no-trackTypeOnlyDependencies --showCircularDependencies --displayMode=raw -e .ts",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:debug": "vitest --inspect-brk --test-timeout=0 --no-file-parallelism",
    "test:seq": "vitest run --no-file-parallelism"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.8",
    "@webda/cache": "^4.0.0-beta.1",
    "@webda/models": "^4.0.0-beta.1",
    "@webda/ql": "^4.0.0-beta.2",
    "@webda/utils": "^4.0.0-beta.1",
    "@webda/workout": "^4.0.0-beta.2",
    "accept-language": "^3.0.18",
    "ajv": "^8.6.2",
    "ajv-formats": "^3.0.1",
    "antlr4ts": "^0.5.0-alpha.4",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "dateformat": "^5.0.3",
    "deepmerge-ts": "^5.1.0",
    "email-templates": "^12.0.1",
    "glob": "^10.0.0",
    "global": "^4.4.0",
    "json-schema": "^0.4.0",
    "jsonc-parser": "^3.0.0",
    "jsondiffpatch": "^0.7.3",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "merge": "^2.1.1",
    "mime-types": "^2.1.31",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.0",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^6.6.3",
    "openapi-types": "^12.0.0",
    "pem-jwk": "^2.0.0",
    "prom-client": "^15.0.0",
    "sanitize-html": "^2.4.0",
    "semver": "^7.3.5",
    "stream-buffers": "^3.0.2",
    "ts-json-schema-generator": "^2.3.0",
    "ts-method-cache": "^3.3.1",
    "uri-templates": "^0.2.0",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/node": "22.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/sanitize-html": "^2.9.5",
    "@vitest/coverage-istanbul": "^2.1.2",
    "@vitest/coverage-v8": "2.1.2",
    "@webda/compiler": "^4.0.0-beta.2",
    "axios": "^1.1.3",
    "c8": "^10.1.2",
    "eslint": "^9.12.0",
    "fs-extra": "^11.0.0",
    "jest": "^29.7.0",
    "jscodeshift": "^17.0.0",
    "mocha": "^10.0.0",
    "prettier": "^3.3.3",
    "sinon": "^19.0.2",
    "skott": "^0.35.3",
    "source-map-support": "^0.5.19",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2",
    "ws": "^8.13.0"
  },
  "files": [
    "lib",
    "test",
    "webda.module.json"
  ],
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "exclude": [
      "hooks.mjs",
      "hooks-register.mjs",
      "lib/**/*",
      "src/test.ts",
      "**/*.spec.ts",
      "test/**/*",
      "*/stores/webdaql/WebdaQL*"
    ],
    "excludeNodeModules": true
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "implicitDependencies": [
      "@webda/tsc-esm",
      "@webda/workout"
    ]
  },
  "webda": {
    "namespace": "Webda"
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "license": "LGPL-3.0-only"
}