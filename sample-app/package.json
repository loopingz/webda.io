{
  "name": "@webda/sample-app",
  "version": "3.999.0",
  "description": "Simple app used for deployment test",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git://github.com/loopingz/webda.io.git",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "webdac",
    "pretest": "npm run build",
    "test": "echo 'No test sample available yet'",
    "format": "prettier --check src/**/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@webda/aws": "^4.0.0-beta.1",
    "@webda/compiler": "^4.0.0-beta.1",
    "@webda/core": "^4.0.0-beta.1",
    "deepmerge": "^4.2.2",
    "merge": "^2.1.1",
    "yaml": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "webda": {
    "logo": "none.txt",
    "namespace": "WebdaDemo"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "tsx": "^4.17.0",
    "skott": "^0.35.3",
    "jscodeshift": "^17.0.0",
    "@webda/compiler": "^4.0.0-beta.2"
