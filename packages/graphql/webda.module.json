{
  "moddas": {
    "MyCompany/GraphQLService": "lib/graphql:GraphQLService"
  },
  "beans": {},
  "models": {},
  "deployers": {},
  "schemas": {
    "MyCompany/GraphQLService": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "description": "Type of the service"
        },
        "url": {
          "type": "string",
          "description": "URL on which to serve the content"
        },
        "operations": {
          "type": "boolean",
          "description": "Expose objects as operations too"
        },
        "nameTransfomer": {
          "type": "string",
          "enum": [
            "camelCase",
            "lowercase",
            "none"
          ]
        },
        "openapi": {
          "type": "object",
          "additionalProperties": true
        }
      },
      "required": [
        "nameTransfomer",
        "operations",
        "type"
      ],
      "description": "Parameters for the GraphQL service",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "GraphQLService"
    }
  }
}