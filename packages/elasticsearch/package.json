{
  "name": "@webda/elasticsearch",
  "version": "4.0.0-beta.1",
  "description": "Webda ElasticSearch implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "webda build",
    "build:watch": "webda build --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "pretest": "npm run build",
    "test": "c8 mocha",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda",
    "modda"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "author": "loopingz",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^8.1.0",
    "@webda/core": "^4.0.0-beta.1",
    "dateformat": "^5.0.3",
    "yaml": "^2.0.0"
  },
  "c8": {
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "report-dir": "./reports",
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/node": "22.0.0",
    "@webda/shell": "^4.0.0-beta.1",
    "sinon": "^19.0.2",
    "tsx": "^4.17.0",
    "skott": "^0.35.3",
    "jscodeshift": "^17.0.0",
    "@webda/compiler": "^4.0.0-beta.2"
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "nx": {
    "implicitDependencies": [
      "@webda/core",
      "@webda/shell"
    ]
  }
}