{
  "name": "@webda/elasticsearch",
  "version": "2.4.2",
  "description": "Webda ElasticSearch implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "webda build",
    "build:watch": "webda build --watch",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*",
    "pretest": "npm run build",
    "test": "c8 mocha --recursive --exit --timeout=100000 src/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda",
    "modda"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "author": "loopingz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^8.1.0",
    "@webda/core": "^2.4.2",
    "yaml": "^2.0.0"
  },
  "c8": {
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "report-dir": "./reports",
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/node": "14.6.0",
    "@webda/shell": "^2.4.2",
    "sinon": "^15.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  }
}