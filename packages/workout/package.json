{
  "name": "@webda/workout",
  "version": "2.1.2",
  "description": "Abstraction to loggin and user interaction",
  "keywords": [
    "terminal",
    "logger"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc-esm",
    "pretest": "npm run build",
    "lint": "prettier --check src/**/*",
    "lint:fix": "prettier --write src/**/*",
    "test": "c8 mocha --recursive --exit --timeout=100000 src/**/*.spec.ts src/*.spec.ts"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "path": "^0.12.7",
    "sprintf-js": "^1.1.2",
    "uuid": "^9.0.0",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.0",
    "@types/json-schema": "^7.0.8",
    "@types/node": "=14.6.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/uuid": "^8.3.4",
    "@webda/tsc-esm": "^1.0.1",
    "c8": "^7.11.3",
    "mocha": "^10.0.0",
    "mock-stdin": "^1.0.0",
    "prettier": "^2.3.2",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "~4.8.2"
  },
  "files": [
    "lib"
  ],
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=14.6.0"
  }
}
