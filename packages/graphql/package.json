{
  "name": "@webda/graphql",
  "version": "2.4.3",
  "description": "GraphQL endpoint for your Webda Application",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "webda build",
    "pretest": "npm run build",
    "test": "c8 mocha --recursive --exit --timeout=100000 -r ../../node_modules/ts-node/register src/**/*.spec.ts",
    "test:debug": "mocha --recursive --exit --timeout=100000 src/**/*.spec.ts src/*.spec.ts",
    "new-model": "webda init webda:model",
    "new-service": "webda init webda:service",
    "lint": "lerna run lint --parallel",
    "lint:fix": "lerna run lint:fix --parallel"
  },
  "keywords": [
    "webda-module"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "dependencies": {
    "@types/node": "^18.15.3",
    "@webda/core": "^2.4.2",
    "graphql": "^16.6.0",
    "graphql-http": "^1.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@webda/shell": "^2.4.3",
    "c8": "^7.13.0",
    "glob": "^9.3.0",
    "js-beautify": "^1.14.7",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "c8": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "exclude": [
      "**/*.spec.ts",
      "test/**/*"
    ]
  },
  "webda": {
    "namespace": "MyCompany"
  },
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda.io#readme",
  "repository": "git://github.com/loopingz/webda.io.git",
  "license": "MIT"
}
