{
  "name": "@webda/workout",
  "version": "4.0.0-beta.1",
  "description": "Abstraction to loggin and user interaction",
  "keywords": [
    "terminal",
    "logger"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc-esm",
    "pretest": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "c8 mocha",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts"
  },
  "dependencies": {
    "yoctocolors": "^2.1.1",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.8",
    "@types/node": "20.0.0",
    "@types/sprintf-js": "^1.1.2",
    "@webda/tsc-esm": "^4.0.0-beta.1",
    "@webda/test": "^4.0.0-beta.0",
    "c8": "^10.1.2",
    "mocha": "^10.0.0",
    "mock-stdin": "^1.0.0",
    "prettier": "^3.0.0",
    "sinon": "^19.0.2",
    "skott": "^0.35.3",
    "source-map-support": "^0.5.19",
    "@inquirer/prompts": "^7.0.0"
  },
  "files": [
    "lib"
  ],
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "nx": {
    "implicitDependencies": [
      "@webda/tsc-esm"
    ]
  },
  "license": "LGPL-3.0-only",
  "optionalDependencies": {
    "@inquirer/prompts": "^7.0.0"
  }
}
