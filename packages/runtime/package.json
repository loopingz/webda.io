{
  "name": "@webda/runtime",
  "version": "4.0.0-beta.2",
  "description": "@webda runtime services",
  "keywords": [],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.io.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "webda build",
    "build:watch": "webda build --watch",
    "pretest": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "c8 mocha",
    "test:debug": "mocha --no-timeouts",
    "format": "prettier --check src/**/*",
    "format:fix": "prettier --write src/**/*",
    "deps:graph": "skott --showCircularDependencies --displayMode=raw -e .ts",
    "codemod": "jscodeshift --parser=ts --extensions=tsx,ts -t ../codemod/current.mjs src"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.8",
    "@webda/core": "^4.0.0-beta.2",
    "@webda/ql": "^4.0.0-beta.2",
    "@webda/workout": "^4.0.0-beta.2",
    "accept-language": "^3.0.18",
    "ajv": "^8.6.2",
    "ajv-formats": "^3.0.1",
    "antlr4ts": "^0.5.0-alpha.4",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "dateformat": "^5.0.3",
    "deepmerge-ts": "^5.1.0",
    "email-templates": "^12.0.1",
    "glob": "^10.0.0",
    "global": "^4.4.0",
    "json-schema": "^0.4.0",
    "jsonc-parser": "^3.0.0",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "merge": "^2.1.1",
    "mime-types": "^2.1.31",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.0",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^6.6.3",
    "openapi-types": "^12.0.0",
    "password-prompt": "^1.1.3",
    "pem-jwk": "^2.0.0",
    "prom-client": "^15.0.0",
    "sanitize-html": "^2.4.0",
    "semver": "^7.3.5",
    "stream-buffers": "^3.0.2",
    "ts-method-cache": "^3.3.1",
    "uri-templates": "^0.2.0",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.2",
    "@types/node": "22.0.0",
    "@webda/shell": "^4.0.0-beta.2",
    "c8": "^10.1.2",
    "fs-extra": "^11.0.0",
    "mocha": "^10.0.0",
    "prettier": "^3.0.0",
    "sinon": "^19.0.2",
    "source-map-support": "^0.5.19",
    "typescript": "^5.6.2",
    "ws": "^8.13.0",
    "tsx": "^4.17.0",
    "skott": "^0.35.3",
    "jscodeshift": "^17.0.0",
    "@webda/compiler": "^4.0.0-beta.2"
  },
  "files": [
    "lib",
    "webda.module.json"
  ],
  "c8": {
    "report-dir": "./reports",
    "reporter": [
      "html",
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "src/test.ts",
      "**/*.spec.ts",
      "test/**/*",
      "*/stores/webdaql/WebdaQL*",
      "lib/**"
    ],
    "excludeNodeModules": true
  },
  "homepage": "https://webda.io",
  "publishConfig": {
    "access": "public"
  },
  "webda": {
    "namespace": "Webda"
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "license": "LGPL-3.0-only"
